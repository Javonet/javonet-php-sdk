{
    "name": "javonet/javonet-php-sdk",
    "description": "Javonet allows you to reference and use modules or packages written in (Java/Kotlin/Groovy/Clojure, C#/VB.NET, Ruby, Perl, Python, PHP, JavaScript/TypeScript) like they were created in your technology.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-ffi": "*",
        "ext-zip": "*",
        "guzzlehttp/promises": "^2.0"
    },
    "autoload": {
        "psr-4": {
          "sdk\\": "javonet/sdk/",
          "utils\\": "javonet/utils/",
          "core\\": "javonet/core/"
        }
    },
    "authors": [
        {
            "name": "Javonet Team",
            "email": "support@javonet.com",
            "homepage": "https://www.javonet.com"
        }
    ],
    "keywords": [
        "javonet",
        "javonet-php",
        "php-javonet",
        "php",
        "cross-platform",
        "integration"
    ],
    "support": {
        "docs": "https://www.javonet.com/guides/v2/",
        "email": "support@javonet.com"
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": [
            "@php scripts/extract-zip-packages.php"
        ],
        "post-update-cmd": [
            "@post-install-cmd"
        ]
    },
    "extra": {
        "javonet": {
            "sdk-type": "public",
            "dependencies": {
                "binaries": "zip",
                "core": "zip",
                "utils": "zip",
                "sdk": "zip"
            }
        }
    }
}
